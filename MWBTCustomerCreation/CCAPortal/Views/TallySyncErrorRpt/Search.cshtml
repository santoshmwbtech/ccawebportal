@model WBT.DLCustomerCreation.TallySyncErrorDTO

@using (Ajax.BeginForm("Search", "TallySyncErrorRpt", null, new AjaxOptions
{
    UpdateTargetId = "divList",
    OnSuccess = "onSearchSuccess",
    HttpMethod = "post",
    LoadingElementId = "progress"

}, new { @class = "form-sample" }))
{
    @Html.AntiForgeryToken()

    <div class="row">
        <div class="col-md-12">
            <div class="card card-primary">
                <div class="card-header">
                    <h3 class="card-title">Tally Sync Errors</h3>
                    <div class="card-tools">
                        <button type="button" class="btn btn-tool" data-card-widget="collapse">
                            <i class="fas fa-minus"></i>
                        </button>
                        <button type="button" class="btn btn-tool" data-card-widget="remove">
                            <i class="fas fa-times"></i>
                        </button>
                    </div>
                </div>
                <div class="card-body">
                    <div class="row">
                        <div class="col-md-4">
                            <div class="form-group">
                                
                            </div>
                        </div>
                        <div class="col-md-4">
                            <div class="form-group">
                                <label> &nbsp; </label>
                                <div class="input-group">
                                    @Html.EditorFor(model => model.CreatedDateStr, new { htmlAttributes = new { @class = "form-control dated", @autocomplete = "off" } })
                                </div>
                            </div>
                        </div>
                        <div class="col-md-4">
                            <div class="form-group">
                                
                            </div>
                        </div>
                    </div>
                    <div class="card-footer">
                        <div class="text-center">
                            <button type="submit" class="btn btn-primary">Search</button>
                        </div>
                    </div>

                </div>
            </div>
        </div>
    </div>
}
@section scripts {
    <script>
        $.validator.unobtrusive.parse(document);
    </script>

}
