@model IEnumerable<WBT.DLCustomerCreation.CustomerVendorDetails>

        <div class="row">
            <div class="col-12">
                <div>
                    <div class="card card-primary">
                        <div class="card-header">
                            <h3 class="card-title"> Customers List </h3>
                            <div class="card-tools">
                                <button type="button" class="btn btn-tool" data-card-widget="collapse"><i class="fas fa-minus"></i></button>
                                <button type="button" class="btn btn-tool" data-card-widget="remove"><i class="fas fa-times"></i></button>
                            </div>
                        </div>
                        <div class="card-body">
                            <div>
                                <table class="table table-bordered table-striped" id="Subtable">
                                    @*dataGrid*@
                                    <thead>
                                        <tr>
                                            <th>
                                                Customer Name
                                            </th>
                                            <th style="width:10px;" class="noExport">

                                                CheckAll<input type="checkbox" id='checkall' />
                                            </th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        @if (Model != null)
                                        {
                                            if (Model.Count() > 0)
                                            {
                                                foreach (var item in Model)
                                                {
                                                    <tr>

                                                        <td>
                                                            @item.FirmName

                                                        </td>
                                                        <td>
                                                            <input type="checkbox" name="checkboxs">
                                                        </td>
                                                    </tr>
                                                }
                                            }
                                        }
                                    </tbody>
                                </table>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>